{
    "name": "daerentis/laravel-version",
    "description": "This package provides a simple way to get the current Laravel version and send it to an external API.",
    "version": "1.0.0",
    "keywords": [
        "daerentis",
        "laravel",
        "laravel-version"
    ],
    "homepage": "https://github.com/daerentis/laravel-version",
    "license": "MIT",
    "authors": [
        {
            "name": "Christian Strausbach",
            "email": "christian.strausbach@freshlime.de",
            "homepage": "https://www.freshlime.de",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Daerentis\\LaravelVersion\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Daerentis\\LaravelVersion\\LaravelVersionServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
